  body
    include ./layouts/header

    main
      .container-fluid
        .row
          .col-lg-6.map-area
              iframe#map(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d26957.595644419718!2d-111.0088491619401!3d32.373607070961825!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x86d60c9846703bc7%3A0x4e4e1dafec9eb44c!2zUmFuY2hvIEZlbGl6LCDQntGA0L4t0JLQtdC50LvRliwg0JDRgNGW0LfQvtC90LAsINCh0L_QvtC70YPRh9C10L3RliDQqNGC0LDRgtC4INCQ0LzQtdGA0LjQutC4!5e0!3m2!1suk!2sua!4v1690477582523!5m2!1suk!2sua", width="100%", height="100%", style="border:0;", allowfullscreen="", loading="lazy", referrerpolicy="no-referrer-when-downgrade")
          .col-lg-6
            .content-area#addressArea
              .mb_30
                a(href="#") < Back to Photos 
              .d-xl-flex.justify-content-between.align-items-center.mb_30
                h1.mb-xl_0 Listing address
                .d-flex.align-items-center
                  | Subtotal
                  span.text-black.fs-25.ms_5 $399
              .row.form-item
                .col-md-4.mb_15.pe-md_0.form-item.form-item-shown
                  input.form-control(type="text", placeholder="Street#")
                .col-md-5.mb_15.pe-md_0.form-item.form-item-shown
                  input.form-control(type="text", placeholder="Street name")
                .col-md-3.mb_15.pe-md_0.form-item.form-item-shown
                  input.form-control(type="text", placeholder="Unit/apt #")
              .row.form-item
                .col-md-3.pe-md_0.mb_15.form-item.form-item-shown
                  input.form-control(type="text", placeholder="City")
                .col-md-3.pe-md_0.mb_15.form-item.form-item-shown
                  input.form-control(type="text", placeholder="State")
                .col-md-3.pe-md_0.mb_15.form-item.form-item-shown
                  input.form-control(type="text", placeholder="Zip")
                .col-md-3.pe-md_0.mb_15.form-item.form-item-shown
                  input.form-control(type="text", placeholder="Country")
              .form-item.form-item-in.height-0
                h3.h2 Property type
                select.form-control.mb_15.mb-sm_0 
                  option(value="0", selected="selected") Select property type
                  option(value="1") Single Family Home
                  option(value="3") Condo / Apt / Townhome
                  option(value="7") Boat / Yacht
                  option(value="19") Auto / Motorhome
              .form-item.form-item-in.height-0
                h3.h2 Size
                input.form-control.mb_15.mb-sm_0(type="text", placeholder="3 bedrooms ($199 + photos)")
              .form-item.form-item-in.height-0
                h3.h2 Use license
                input.form-control.mb_15.mb-sm_0(type="text", placeholder="Single listing ($0)")

              .form-item.form-item-in.height-0#shootDateArea
                h3.h2 Shoot Date
                .mb_30 Weâ€™ll book closest to this and confirm today.

                .d-sm-flex.mb_15
                  .form-control-medium-2.mb_15.mb-sm_0
                    input.form-control(type="date")
                  .form-control-medium.mb_15.mb-sm_0.ms-sm_9
                    input.form-control(type="time")
                  .mb-sm_0.ms-sm_9.w-100
                    input.form-control(type="text", value="Photos ready next day ($0)")
                .mb_5 Alternate Date/Time
                .d-sm-flex.mb_15
                  .form-control-medium-2.mb_15.mb-sm_0
                    input.form-control(type="date")
                  .form-control-medium.mb_15.mb-sm_0.ms-sm_9
                    input.form-control(type="time")
                  .mb-sm_0.ms-sm_9.w-100
                    input.form-control(type="text", value="Photos ready next day ($0)")
                .form-item-in
                  h3.h2 Will you attend?
                  .mb_10
                    input.form-control(type="text", value="No. I give permission to enter (most common)")
                  .mb_10
                    textarea.form-control(cols="30", rows="5") Lockbox code # and any additional shoot instructions here.
                .form-item-in
                  h3.h2 Login
                  .d-sm-flex.gap-10
                    .mb_15
                      input.form-control(type="email", placeholder="Email")
                    .mb_15
                      input.form-control(type="password", placeholder="Password")
                
              .mt_35#nextBtnArea
                button#nextBtn.btn.btn-primary(disabled="disabled") Next

              .mt_20.text-center.text-sm-start.d-none#loginBtnArea
                button.btn.btn-primary.btn-medium-2 Sign In
                button.btn.btn-secondary.btn-medium-2.ms_10 New User?

